#include<iostream>
using namespace std;
int main()
{
  int temp=0 ,first=0, second=1, i, n, sum=0;
  cin>>n;
  for(i=0 ; i<n ; i++)
  {  
    if(i <= 1)
    {
      sum=i;
    }
    // to print 0 and 1
    else
    {
      sum=first + second;
      first=second;
      second=sum;  
    }
    temp=sum;
  }
  cout<<temp;


}
